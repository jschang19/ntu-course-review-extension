{
    "manifest_version": 3,
    "name": "台大課程評價小幫手",
    "description": "輕鬆查詢台大課程評價",
    "version": "1.0.1",
    "author": "jschang19",
    "action": {
        "default_popup": "popup.html",
        "default_title": "台大課程評價小幫手",
        "default_icon": "icon.png"
    },
    "icons": {
        "128": "icon_128.png"
    },
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "https://ntucourse-api.jcshawn.com/api/course/*"
    ]
}